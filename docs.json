{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "TensorPix API docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
          "group": "Endpoints",
          "openapi": {
            "source": "https://backend.tensorpix.ai/api/schema/",
            "directory": "api-reference"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Help Center",
          "href": "https://help.tensorpix.ai",
          "icon": "square-question"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/672xZfkVkt",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://tensorpix.ai/blog",
          "icon": "newspaper"
        }
        {
          "anchor": "Web Application",
          "href": "https://app.tensorpix.ai",
          "icon": "globe"
        },
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:devs@tensorpix.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Web Application",
      "href": "https://app.tensorpix.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tensorpix",
      "github": "https://github.com/tensorpix",
      "linkedin": "https://linkedin.com/company/tensorpix",
      "instagram": "https://instagram.com/tensorpix.ai" 
    }
  }
}